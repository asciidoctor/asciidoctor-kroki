{
  "name": "asciidoctor-kroki",
  "version": "0.15.4",
  "description": "Asciidoctor extension to convert diagrams to images using Kroki",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/asciidoctor-kroki.cjs",
  "module": "dist/asciidoctor-kroki.module.js",
  "umd:main": "dist/asciidoctor-kroki.umd.js",
  "unpkg": "dist/asciidoctor-kroki.umd.js",
  "exports": {
    ".": {
      "browser": "./dist/asciidoctor-kroki.module.js",
      "umd": "./dist/asciidoctor-kroki.umd.js",
      "import": "./dist/asciidoctor-kroki.js",
      "require": "./dist/asciidoctor-kroki.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "node tasks/build.js",
    "test": "npm run build && npm run test:node && npm run test:browser && npm run test:antora",
    "test:node": "mocha 'test/**.spec.js'",
    "test:browser": "node test/browser/run.js",
    "test:antora": "mocha 'test/antora/**.spec.cjs'",
    "lint": "standard src/**.js test/**.js tasks/**.js",
    "lint-fix": "npm run lint -- --fix",
    "clean": "shx rm -rf dist/*",
    "dist": "npm run clean && npm run build"
  },
  "dependencies": {
    "json5": "2.2.0",
    "mkdirp": "1.0.4",
    "pako": "2.0.4",
    "unxhr": "1.2.0"
  },
  "devDependencies": {
    "@antora/site-generator-default": "~3.0",
    "@asciidoctor/core": "~2.2",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "3.0.1",
    "base64-js": "1.5.1",
    "chai": "4.3.6",
    "chai-string": "1.5.0",
    "cheerio": "1.0.0-rc.10",
    "dirty-chai": "2.0.1",
    "libnpmpublish": "4.0.2",
    "lodash": "4.17.21",
    "mocha": "9.2.0",
    "pacote": "12.0.2",
    "puppeteer": "13.1.3",
    "rimraf": "3.0.2",
    "rollup": "2.67.0",
    "rollup-plugin-terser": "7.0.2",
    "shx": "0.3.4",
    "sinon": "13.0.1",
    "standard": "16.0.4"
  },
  "peerDependencies": {
    "@asciidoctor/core": "~2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mogztter/asciidoctor-kroki.git"
  },
  "keywords": [
    "asciidoctor",
    "kroki",
    "diagrams",
    "javascript",
    "extension"
  ],
  "author": "Guillaume Grossetie (https://github.com/mogztter)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mogztter/asciidoctor-kroki/issues"
  },
  "homepage": "https://github.com/Mogztter/asciidoctor-kroki#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  }
}
